{
    "compilerOptions": {
        "incremental": false,
        "target": "es2020",
        "module": "commonjs",
        "rootDir": "./",
        "sourceMap": false,
        "outDir": "./dist",
        "removeComments": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "pretty": true,
        "resolveJsonModule": true
    },
    "include": [
        "./src/**/*",
    ]
}